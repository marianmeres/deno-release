{
  "name": "@marianmeres/deno-release",
  "version": "1.3.1",
  "exports": "./deno-release.ts",
  "tasks": {
    "release": "deno run -A deno-release.ts",
    "publish": "deno publish",
    "rp": "deno task release -y && deno publish",
    "rpm": "deno task release minor -y && deno publish"
  },
  "imports": {},
  "fmt": {
    "useTabs": true,
    "lineWidth": 90,
    "indentWidth": 4,
    "proseWrap": "preserve"
  }
}
